#全局日志配置
#日志存储路径，不存在时会自动创建，默认“./log”
log_path = "./log"

#日志实时输出开关 0：关闭，1：开启
log_flush = 1

#日志文件保存数量，超过数量会滚动删除，配置范围[0, 65535]
log_file_count = 5

#日志文件单个大小，单位：MB，超过大小切换文件，配置范围（0, 65535]
log_file_size = 30

#日志预留空间大小，单位：MB，达到阈值日志输出到终端，配置范围[0, +∞)
log_reserve_space = 200

#各模块日志配置，有几个模块配置几个
#日志输出类型：0：off，1：console，2：file，4：serial，8：net，可叠加
log_A_output = 2
log_B_output = 2
log_C_output = 2
log_D_output = 2

#日志等级类型：0：trace，1：debug，2：info，3：warn，4：error，5：fatal，6：close
log_A_level = 2
log_B_level = 2
log_C_level = 2
log_D_level = 2
